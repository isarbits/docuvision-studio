{
    "name": "docuvision-studio-server",
    "version": "0.0.1",
    "private": true,
    "description": "Simple backend for client",
    "author": "Isarbits GmbH",
    "license": "MIT",
    "scripts": {
        "preinstall": "node .npm-scripts/preinstall.js",
        "postbuild": "cp .env.example dist/.env",
        "prebuild": "rimraf dist",
        "build": "tsc -p tsconfig.build.json",
        "format": "prettier --write -l \"src/**/*.ts\" && eslint --fix src/ --ext .ts",
        "watch": "npm run build -- -w",
        "start:dev": "tsc-watch --preserveWatchOutput --onSuccess 'npm start'",
        "start": "cd dist && node src/main.js"
    },
    "dependencies": {
        "@elastic/elasticsearch": "^7.6.1",
        "@nestjs/bull": "^0.1.0",
        "@nestjs/common": "^7.0.7",
        "@nestjs/config": "^0.4.0",
        "@nestjs/core": "^7.0.7",
        "@nestjs/elasticsearch": "^7.1.0",
        "@nestjs/microservices": "^7.0.7",
        "@nestjs/platform-express": "^7.0.7",
        "@nestjs/platform-ws": "^7.0.7",
        "@nestjs/schedule": "^0.3.0",
        "@nestjs/serve-static": "^2.1.0",
        "@nestjs/websockets": "^7.0.7",
        "body-parser": "^1.19.0",
        "bull": "^3.13.0",
        "class-transformer": "^0.2.3",
        "class-validator": "^0.11.1",
        "config": "^3.3.1",
        "dotenv": "^8.2.0",
        "form-data": "^3.0.0",
        "http-proxy-middleware": "^1.0.3",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^6.5.5",
        "source-map-support": "^0.5.16",
        "uuid": "^7.0.3",
        "ws": "^7.2.3"
    },
    "devDependencies": {
        "@types/bull": "^3.12.1",
        "@types/express": "^4.17.4",
        "@types/node": "^13.11.0",
        "@types/ws": "^7.2.3",
        "@typescript-eslint/eslint-plugin": "^2.27.0",
        "@typescript-eslint/parser": "^2.27.0",
        "eslint": "^6.8.0",
        "eslint-plugin-simple-import-sort": "^5.0.2",
        "minimist": "^1.2.6",
        "prettier": "^2.0.4",
        "rimraf": "^3.0.2",
        "tsc-watch": "^4.1.0",
        "typescript": "^3.7.5"
    },
    "engines": {
        "node": ">=12",
        "npm": ">=6"
    }
}
